{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "node --import=./tsnode.esm.js node_modules/@nestjs/cli/bin/nest.js start --watch",
    "start:debug": "node --import=./tsnode.esm.js --inspect-brk node_modules/@nestjs/cli/bin/nest.js start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "node --import=./tsnode.esm.js node_modules/.bin/jest",
    "test:watch": "node --import=./tsnode.esm.js node_modules/.bin/jest --watch",
    "test:cov": "node --import=./tsnode.esm.js node_modules/.bin/jest --coverage",
    "test:debug": "node --import=./tsnode.esm.js --inspect-brk node_modules/.bin/jest --runInBand",
    "test:e2e": "node --import=./tsnode.esm.js node_modules/.bin/jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/schedule": "^5.0.1",
    "@semaphore-protocol/identity": "^4.10.0",
    "@types/crypto-js": "^4.2.2",
    "@types/multer": "^1.4.13",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "argon2": "^0.41.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.5.0",
    "ethers": "^6.13.5",
    "fastify": "^5.3.3",
    "firebase-admin": "^13.4.0",
    "form-data": "^4.0.3",
    "multer": "^2.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.30",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapping": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}
